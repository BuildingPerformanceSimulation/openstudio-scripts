<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>ReportingMeasure</title>
<link href="http://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.3.2/css/bootstrap.min.css" rel="stylesheet">
<script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
<script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/d3/3.3.9/d3.min.js"></script>
</head>
<body>

<div id="chartContainer">
  <script src="/lib/d3.v4.3.0.js"></script>
  <script src="http://dimplejs.org/dist/dimple.v2.3.0.min.js"></script>
  <script type="text/javascript">
    var svg = dimple.newSvg("#chartContainer", 590, 400);
    d3.tsv("scenario_results.csv", function (data) {
      var myChart = new dimple.chart(svg, data);
      myChart.setBounds(90, 50, 480, 310)
      myChart.addCategoryAxis("x", ["Scenario", "Largest Contributor to Absolute End Use Error"]);
      myChart.addCategoryAxis("y", "Description");
      myChart.addSeries("Largest Contributor to Absolute End Use Error", dimple.plot.bar);
      myChart.addLegend(240, 10, 330, 20, "right");
      myChart.draw();
    });
  </script>
</div>

<%= output.join('') %>
</div>

</body>
</html>
